{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@components/*": ["./src/components/*"],
      "@interfaces/*": ["./src/interfaces/*"],
      "@composables/*": ["./src/composables/*"],
      "@assets/*": ["./src/assets/*"],
      "@utility/*": ["./src/utility/*"]
    }
  }
}
